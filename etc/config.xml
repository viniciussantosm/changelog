<?xml version="1.0"?>
<config>
    <modules>
        <BIS2BIS_Changelog>
            <version>1.0.0</version>
        </BIS2BIS_Changelog>
    </modules>
    <global>
        <blocks>
            <changelog>
                <class>BIS2BIS_Changelog_Block</class>
            </changelog>
            <adminhtml>
                <rewrite>
                    <dashboard>BIS2BIS_Changelog_Block_Adminhtml_Rewrite_Dashboard</dashboard>
                </rewrite>
            </adminhtml>
        </blocks>
        <helpers>
            <changelog>
                <class>BIS2BIS_Changelog_Helper</class>
            </changelog>
        </helpers>
        <models>
            <changelog>
                <class>BIS2BIS_Changelog_Model</class>
                <resourceModel>changelog_resource</resourceModel>
            </changelog>
            <changelog_resource>
                <class>BIS2BIS_Changelog_Model_Resource</class>
                <entities>
                    <log>
                        <table>changelog_logs</table>
                    </log>
                </entities>
            </changelog_resource>
        </models>
        <!-- R E S O U R C E S -->
        <resources>
            <changelog_setup>
                <setup>
                    <module>BIS2BIS_Changelog</module>
                    <class>Mage_Eav_Model_Entity_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </changelog_setup>
            <changelog_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </changelog_write>
            <changelog_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </changelog_read>
        </resources>
        <!-- R E S O U R C E S -->
    </global>
    <adminhtml>
        <translate>
            <modules>
                <BIS2BIS_Changelog>
                    <files>
                        <default>BIS2BIS_Changelog.csv</default>
                    </files>
                </BIS2BIS_Changelog>
            </modules>
        </translate>
    </adminhtml>
</config>